vetor_teste=c(12,4,10,23)
estatisticas_vetor <- function(lista){
    media=0
    mediana=median(lista)
    maiorvalor=0
    menorvalor=99999999999
    for (i in lista) {
        media= media+i/length(lista)
        
        if (i>maiorvalor){
            maiorvalor=i
        }
        if (i<menorvalor){
            menorvalor=i
        }
    }
    return (c(media,mediana,maiorvalor,menorvalor))
}


print(estatisticas_vetor(vetor_teste))
# Exemplo de uso:
#crie o vetor_teste
#calcule o resultado usando o vetor da funcao e selve como resultado


